{
    "name": "mcp-manager",
    "version": "1.1.0",
    "description": "Gerenciador local para servidores MCP com clonagem Git centralizada",
    "main": "server.js",
    "bin": "server.js",
    "scripts": {
      "start": "node server.js",
      "test": "echo \"Error: no test specified\" && exit 1",
      "build-pkg": "pkg . --targets node18-win-x64,node18-macos-x64,node18-linux-x64 --output dist/mcp-manager"
    },
    "keywords": [
      "mcp",
      "manager",
      "server",
      "git"
    ],
    "author": "Seu Nome",
    "license": "ISC",
    "dependencies": {
      "cors": "^2.8.5",
      "express": "^4.18.2",
      "uuid": "^9.0.1",
      "ws": "^8.14.2"
    },
    "devDependencies": {
      "pkg": "^5.8.1"
    },
    "pkg": {
      "assets": [
        "public/**/*",
        "servers.json"
      ],
      "targets": [
        "node18-win-x64",
        "node18-macos-x64",
        "node18-linux-x64"
      ],
      "outputPath": "dist"
    }
  }